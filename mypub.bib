@article{benhabib_steady-state_2008,
	title = {Steady-state diffusion regime in solid-phase microextraction kinetics},
	volume = {609},
	issn = {1873-4324 (Electronic) 0003-2670 (Linking)},
	shorttitle = {Steady-state diffusion regime in solid-phase microextraction kinetics},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/18243879},
	doi = {10.1016/j.aca.2007.11.053},
	abstract = {The temporal evolution of diffusion-controlled analyte accumulation in solid-phase microextraction (SPME) is critically discussed in terms of the various aspects of steady-state diffusion in the two phases under conditions of fast exchange of the analyte at the solid phase film/water interface. For partition coefficients (K(sw)) much larger than unity and a sufficiently thin polymer film, the concentration gradient of the analyte in the polymer phase is largely insignificant. The growth of the accumulated amount of analyte is then adequately described by the well-known exponential expression for steady-state diffusion under non-depletive conditions, provided the initial transient stage is properly taken into account. In case of fiber-type solid phases, the cylindrical nature of the diffusion complicates the nature of the transient stage as well as the magnitude of the steady-state flux.},
	number = {1},
