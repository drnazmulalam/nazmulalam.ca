@article{arp_review_2015,
	title = {Review of polyoxymethylene passive sampling methods for quantifying freely dissolved porewater concentrations of hydrophobic organic contaminants},
	volume = {34},
	issn = {1552-8618 (Electronic) 0730-7268 (Linking)},
	shorttitle = {Review of polyoxymethylene passive sampling methods for quantifying freely dissolved porewater concentrations of hydrophobic organic contaminants},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/25702935},
	doi = {10.1002/etc.2864},
	abstract = {Meth ods involving polyoxymethylene (POM) as a passive sampler are increasing in popularity to assess contaminant freely dissolved porewater concentrations in soils and sediments. These methods require contaminant-specific POM-water partition coefficients, KPOM . Certain methods for determining KPOM perform reproducibly (within 0.2 log units). However, other methods can give highly varying KPOM values (up to 2 log units), especially for polycyclic aromatic hydrocarbons (PAHs). To account for this variation, the authors tested the influence of key methodological components in KPOM determinations, including POM thickness, extraction procedures, and environmental temperature and salinity, as well as uptake kinetics in mixed and static systems. All inconsistencies in the peer-reviewed literature can be accounted for by the likelihood that thick POM materials (500 mum or thicker) do not achieve equilibrium (causing negative biases up to 1 log unit), or that certain POM extraction procedures do not ensure quantitative extraction (causing negative biases up to 2 log units). Temperature can also influence KPOM , although all previous literature studies were carried out at room temperature. The present study found that KPOM values at room temperature are independent (within 0.2 log units) of POM manufacture method, of thickness between 17 mum and 80 mum, and of salinity between 0\% and 10\%. Regarding kinetics, monochloro- to hexachloro-polychlorinated biphenyls (PCBs) were within 0.2 log units of equilibrium after 28 d in the mixed system, but only dichloro-PCBs achieved near equilibrium after 126 d in the static system. Based on these insights, recommended methods and KPOM values to facilitate interlaboratory reproducibility are presented.},
	number = {4},
	journal = {Environ Toxicol Chem},
	author = {Arp, H. P. and Hale, S. E. and Elmquist Krusa, M. and Cornelissen, G. and Grabanski, C. B. and Miller, D. J. and Hawthorne, S. B.},
	month = apr,
	year = {2015},
	keywords = {Kinetics, Environmental Monitoring/*methods, Animals, Reproducibility of Results, Temperature, Water Pollutants, Chemical/*analysis, Resins, Synthetic/*chemistry, Algorithms, Water Pollution/*analysis, Geologic Sediments},
	pages = {710--20},
	annote = {Arp, Hans Peter HHale, Sarah EElmquist Krusa, MarieCornelissen, GerardGrabanski, Carol BMiller, David JHawthorne, Steven BENGResearch Support, Non-U.S. Gov'tReview2015/02/24 06:00Environ Toxicol Chem. 2015 Apr;34(4):710-20. doi: 10.1002/etc.2864. Epub 2015 Feb 27.},
	annote = {Arp, Hans Peter HHale, Sarah EElmquist Krusa, MarieCornelissen, GerardGrabanski, Carol BMiller, David JHawthorne, Steven BengResearch Support, Non-U.S. Gov'tReview2015/02/24 06:00Environ Toxicol Chem. 2015 Apr;34(4):710-20. doi: 10.1002/etc.2864. Epub 2015 Feb 27.},
	file = {Arp_et_al-2015-Environmental_Toxicology_and_Chemistry:C\:\\Users\\MTeam\\Zotero\\storage\\2DT75PWR\\Arp_et_al-2015-Environmental_Toxicology_and_Chemistry.pdf:application/pdf;Arp et al-2015-Review of polyoxymethylene passive sampling methods for freely dissolved porewater:C\:\\Users\\MTeam\\Zotero\\storage\\RI44TVGM\\Arp et al-2015-Review of polyoxymethylene passive sampling methods for freely dissolved porewater.pdf:application/pdf}
}

@article{booij_passive_2016,
	title = {Passive {Sampling} in {Regulatory} {Chemical} {Monitoring} of {Nonpolar} {Organic} {Compounds} in the {Aquatic} {Environment}},
	volume = {50},
	issn = {1520-5851 (Electronic) 0013-936X (Linking)},
	shorttitle = {Passive {Sampling} in {Regulatory} {Chemical} {Monitoring} of {Nonpolar} {Organic} {Compounds} in the {Aquatic} {Environment}},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/26619247},
	doi = {10.1021/acs.est.5b04050},
	abstract = {We reviewed compliance monitoring requirements in the European Union, the United States, and the Oslo-Paris Convention for the protection of the marine environment of the North-East Atlantic, and evaluated if these are met by passive sampling methods for nonpolar compounds. The strengths and shortcomings of passive sampling are assessed for water, sediments, and biota. Passive water sampling is a suitable technique for measuring concentrations of freely dissolved compounds. This method yields results that are incompatible with the EU's quality standard definition in terms of total concentrations in water, but this definition has little scientific basis. Insufficient quality control is a present weakness of passive sampling in water. Laboratory performance studies and the development of standardized methods are needed to improve data quality and to encourage the use of passive sampling by commercial laboratories and monitoring agencies. Successful prediction of bioaccumulation based on passive sampling is well documented for organisms at the lower trophic levels, but requires more research for higher levels. Despite the existence of several knowledge gaps, passive sampling presently is the best available technology for chemical monitoring of nonpolar organic compounds. Key issues to be addressed by scientists and environmental managers are outlined.},
	number = {1},
	journal = {Environ Sci Technol},
	author = {Booij, K. and Robinson, C. D. and Burgess, R. M. and Mayer, P. and Roberts, C. A. and Ahrens, L. and Allan, I. J. and Brant, J. and Jones, L. and Kraus, U. R. and Larsen, M. M. and Lepom, P. and Petersen, J. and Profrock, D. and Roose, P. and Schafer, S. and Smedes, F. and Tixier, C. and Vorkamp, K. and Whitehouse, P.},
	month = jan,
