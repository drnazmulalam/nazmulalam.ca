@article{nair_design_2007,
	title = {Design {Considerations} of {Silicon} {Nanowire} {Biosensors}},
	volume = {54},
	issn = {0018-9383},
	doi = {10.1109/TED.2007.909059},
	abstract = {Biosensors based on silicon nanowires (Si-NWs) promise highly sensitive dynamic label-free electrical detection of biomolecules. Despite the tremendous potential and promising experimental results, the fundamental mechanism of electrical sensing of biomolecules and the design considerations of NW sensors remain poorly understood. In this paper, we discuss the prospects and challenges of biomolecule detection using Si-NW biosensors as a function of device parameters, fluidic environment, charge polarity of biomolecules, etc., and refer to experimental results in literature to support the nonintuitive predictions wherever possible. Our results indicate that the design of Si nanobiosensor is nontrivial and as such, only careful optimization supported by numerical simulation would ensure optimal sensor performance.},
	number = {12},
	journal = {IEEE Transactions on Electron Devices},
	author = {Nair, P. R. and Alam, M. A.},
	month = dec,
	year = {2007},
	keywords = {bioelectric phenomena, biomolecule detection, Biosensors, Chemical and biological sensors, Chemical sensors, Design optimization, DNA, electrical sensing, highly sensitive dynamic label-free electrical detection, molecular biophysics, Nanobioscience, nanowires, Poisson–Boltzmann (PB), protein detection, Proteins, random dopant fluctuations, Sensitivity, Sensor arrays, Silicon, silicon nanowire biosensors},
	pages = {3400--3408},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\MTeam\\Zotero\\storage\\92FBJFIC\\4383044.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\MTeam\\Zotero\\storage\\RXKNX8EP\\Nair and Alam - 2007 - Design Considerations of Silicon Nanowire Biosenso.pdf:application/pdf}
}

