	volume = {58},
	issn = {1873-4235 (Electronic) 0956-5663 (Linking)},
	shorttitle = {Dynamic and equilibrium performance of sensors based on short peptide ligands for affinity adsorption of human {IgG} using surface plasmon resonance},
