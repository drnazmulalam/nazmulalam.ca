	pages = {11937--47},
	annote = {Fernandez, Loretta ALao, WenjianMaruya, Keith AWhite, CarmenBurgess, Robert Meng2012/10/16 06:00Environ Sci Technol. 2012 Nov 6;46(21):11937-47. doi: 10.1021/es302139y. Epub 2012 Oct 26.},
	file = {Fernandez et al-2012-Passive sampling to measure baseline dissolved persistent organic:C\:\\Users\\MTeam\\Zotero\\storage\\FNH8G52J\\Fernandez et al-2012-Passive sampling to measure baseline dissolved persistent organic.pdf:application/pdf}
