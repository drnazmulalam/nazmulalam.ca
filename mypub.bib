	pages = {6--14},
	annote = {Roy, BibhasDas, TamalMaiti, Tapas KChakraborty, SumanengResearch Support, Non-U.S. Gov'tNetherlands2011/07/19 06:00Anal Chim Acta. 2011 Sep 2;701(1):6-14. doi: 10.1016/j.aca.2011.05.049. Epub 2011 Jun 6.},
	file = {1-s2.0-S0003267011007677-main:C\:\\Users\\MTeam\\Zotero\\storage\\TEVU2ATB\\1-s2.0-S0003267011007677-main.pdf:application/pdf}
