	annote = {Jimenez-Piedrahita, MartinAltier, AlexandraCecilia, JoanRey-Castro, CarlosGalceran, JosepPuy, JaumeENGNetherlands2015/08/02 06:00Anal Chim Acta. 2015 Jul 23;885:148-55. doi: 10.1016/j.aca.2015.04.054. Epub 2015 May 2.},
	file = {Jimenez2015_inhomogeneous:C\:\\Users\\MTeam\\Zotero\\storage\\CGK3D82S\\Jimenez2015_inhomogeneous.pdf:application/pdf}
}

