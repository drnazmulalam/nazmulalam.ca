@article{plecis_ionic_2011,
	title = {Ionic and mass transport in micro-nanofluidic devices: a matter of volumic surface charge},
	volume = {11},
	issn = {1473-0189},
	shorttitle = {Ionic and mass transport in micro-nanofluidic devices},
	url = {http://pubs.rsc.org/en/content/articlelanding/2011/lc/c0lc00079e},
	doi = {10.1039/C0LC00079E},
	abstract = {The shape and the surface charge of microchannels are critical parameters for ionic and mass transport in microfluidic systems. A great number of studies and developments have been carried out in order to optimize these features separately. We propose to consider them together within a new fundamental parameter for microfluidics, that we named the Volumic Surface Charge (VSC), which is the ratio of the surface charge to the section height in planar microchannels. The non-linear effects induced by rapid VSC variations can result in selective preconcentration processes, which can be used for a simultaneous preconcentration and separation of biomolecules within simple straight channels. In this review, we first present 3 different techniques that we developed to tune the VSC either by surface chemical patterning, integration of polarisable interfaces or geometrical constrictions. The proof of concept of the selective preconcentration using VSC variations will be presented on the basis of experimental results obtained with fluorescent probes and numerical simulations.},
	language = {en},
	number = {5},
	urldate = {2017-01-30},
	author = {Plecis, Adrien and Pallandre, Antoine and Haghiri-Gosnet, Anne-Marie},
	month = mar,
	year = {2011},
	pages = {795--804},
	file = {Snapshot:C\:\\Users\\MTeam\\Zotero\\storage\\I58ZSDSA\\c0lc00079e.html:text/html;Full Text PDF:C\:\\Users\\MTeam\\Zotero\\storage\\RWVQPMGU\\Plecis et al. - 2011 - Ionic and mass transport in micro-nanofluidic devi.pdf:application/pdf}
}

