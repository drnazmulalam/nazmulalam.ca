	pages = {71--6},
	annote = {Qin, ZhipeiMok, SandraOuyang, GangfengDixon, D GeorgePawliszyn, JanuszengResearch Support, Non-U.S. Gov'tNetherlands2010/05/06 06:00Anal Chim Acta. 2010 May 14;667(1-2):71-6. doi: 10.1016/j.aca.2010.04.003. Epub 2010 Apr 8.},
