}

@article{david_stir_2007,
	series = {Advances in {Sample} {Preparation}},
	title = {Stir bar sorptive extraction for trace analysis},
	volume = {1152},
	issn = {0021-9673},
	url = {http://www.sciencedirect.com/science/article/pii/S0021967307000751},
	doi = {10.1016/j.chroma.2007.01.032},
	abstract = {Stir bar sorptive extraction (SBSE) was introduced in 1999 as a solventless sample preparation method for the extraction and enrichment of organic compounds from aqueous matrices. The method is based on sorptive extraction, whereby the solutes are extracted into a polymer coating on a magnetic stirring rod. The extraction is controlled by the partitioning coefficient of the solutes between the polymer coating and the sample matrix and by the phase ratio between the polymer coating and the sample volume. For a polydimethylsiloxane coating and aqueous samples, this partitioning coefficient resembles the octanol–water partitioning coefficient. In comparison to solid phase micro-extraction, a larger amount of sorptive extraction phase is used and consequently extremely high sensitivities can be obtained as illustrated by several successful applications in trace analysis in environmental, food and biomedical fields. Initially SBSE was mostly used for the extraction of compounds from aqueous matrices. The technique has also been applied in headspace mode for liquid and solid samples and in passive air sampling mode. In this review article, the principles of stir bar sorptive extraction are described and an overview of SBSE applications is given.},
	number = {1},
	urldate = {2017-08-24},
	journal = {Journal of Chromatography A},
	author = {David, Frank and Sandra, Pat},
	month = jun,
	year = {2007},
	keywords = {Food, Environmental analysis, Biomedical, Stir bar sorptive extraction, Trace analysis},
	pages = {54--69},
	file = {ScienceDirect Snapshot:C\:\\Users\\MTeam\\Zotero\\storage\\J8N5ISW7\\S0021967307000751.html:text/html;ScienceDirect Full Text PDF:C\:\\Users\\MTeam\\Zotero\\storage\\TE8JQIDZ\\David and Sandra - 2007 - Stir bar sorptive extraction for trace analysis.pdf:application/pdf}
}

@article{koning_modern_2009,
