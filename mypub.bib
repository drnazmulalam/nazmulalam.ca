	file = {Jiang et al-2015 Investigation of the kinetic process of SPME in complex sample:C\:\\Users\\MTeam\\Zotero\\storage\\CGX5IK9F\\Jiang et al-2015 Investigation of the kinetic process of SPME in complex sample.pdf:application/pdf}
