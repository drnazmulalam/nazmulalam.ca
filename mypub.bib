@article{balakrishnan_intravascular_2007,
	title = {Intravascular drug release kinetics dictate arterial drug deposition, retention, and distribution},
	volume = {123},
	issn = {1873-4995 (Electronic) 0168-3659 (Linking)},
	shorttitle = {Intravascular drug release kinetics dictate arterial drug deposition, retention, and distribution},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/17868948},
	doi = {10.1016/j.jconrel.2007.06.025},
	abstract = {Millions of patients worldwide have received drug-eluting stents to reduce their risk for in-stent restenosis. The efficacy and toxicity of these local therapeutics depend upon arterial drug deposition, distribution, and retention. To examine how administered dose and drug release kinetics control arterial drug uptake, a model was created using principles of computational fluid dynamics and transient drug diffusion-convection. The modeling predictions for drug elution were validated using empiric data from stented porcine coronary arteries. Inefficient, minimal arterial drug deposition was predicted when a bolus of drug was released and depleted within seconds. Month-long stent-based drug release efficiently delivered nearly continuous drug levels, but the slow rate of drug presentation limited arterial drug uptake. Uptake was only maximized when the rates of drug release and absorption matched, which occurred for hour-long drug release. Of the two possible means for increasing the amount of drug on the stent, modulation of drug concentration potently impacts the magnitude of arterial drug deposition, while changes in coating drug mass affect duration of release. We demonstrate the importance of drug release kinetics and administered drug dose in governing arterial drug uptake and suggest novel drug delivery strategies for controlling spatio-temporal arterial drug distribution.},
	number = {2},
	journal = {J Control Release},
	author = {Balakrishnan, B. and Dooley, J. F. and Kopia, G. and Edelman, E. R.},
	month = nov,
	year = {2007},
	keywords = {Kinetics, Diffusion, Animals, Reproducibility of Results, Computer Simulation, Solubility, Coronary Vessels/*metabolism, *Stents, Models, Cardiovascular, Cardiovascular Agents/administration \& dosage/chemistry/*metabolism, Chemistry, Pharmaceutical, Delayed-Action Preparations, Drug Compounding, Prosthesis Design, Sirolimus/administration \& dosage/chemistry/*metabolism, Swine},
	pages = {100--8},
	annote = {Balakrishnan, BrindaDooley, John FKopia, GregoryEdelman, Elazer RengR01 GM049039/GM/NIGMS NIH HHS/R01 GM049039-06/GM/NIGMS NIH HHS/R01 HL-49309/HL/NHLBI NIH HHS/Research Support, N.I.H., ExtramuralResearch Support, Non-U.S. Gov'tNetherlands2007/09/18 09:00J Control Release. 2007 Nov 6;123(2):100-8. Epub 2007 Jul 5.},
