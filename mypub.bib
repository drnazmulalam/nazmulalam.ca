@article{chen_microcystin_2007,
	title = {{MICROCYSTIN} {ACCUMULATION} {IN} {FRESHWATER} {BIVALVES} {FROM} {LAKE} {TAIHU}, {CHINA}, {AND} {THE} {POTENTIAL} {RISK} {TO} {HUMAN} {CONSUMPTION}},
