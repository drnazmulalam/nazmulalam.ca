	issn = {15689883},
	url = {http://linkinghub.elsevier.com/retrieve/pii/S1568988315300706},
	doi = {10.1016/j.hal.2016.02.007},
	abstract = {Cyanobacteria can produce toxic metabolites known as cyanotoxins. Common and frequently investigated cyanotoxins include microcystins (MCs), nodularin (NOD) and saxitoxins (STXs). During the summer of 2011 extensive cyanobacterial growth was found in several ﬁshponds in Serbia. Sampling of the water and ﬁsh (common carp, Cyprinus carpio) was performed. Water samples from 13 ﬁshponds were found to contain saxitoxin, microcystin, and/or nodularin. LC–MS/MS showed that MC-RR was present in samples of ﬁsh muscle tissue. Histopathological analyses of ﬁsh grown in ﬁshponds with cyanotoxin production showed histopathological damage to liver, kidney, gills, intestines and muscle tissues. This study is among the ﬁrst so far to report severe hyperplasia of intestinal epithelium and severe degeneration of muscle tissue of ﬁsh after cyanobacterial exposure. These ﬁndings emphasize the importance of cyanobacterial and cyanotoxin monitoring in ﬁshponds in order to recognize cyanotoxins and their potential effects on ﬁsh used for human consumption and, further, on human health.},
	language = {en},
	urldate = {2018-05-30},
	journal = {Harmful Algae},
	author = {Drobac, Damjana and Tokodi, Nada and Lujić, Jelena and Marinović, Zoran and Subakov-Simić, Gordana and Dulić, Tamara and Važić, Tamara and Nybom, Sonja and Meriluoto, Jussi and Codd, Geoffrey A. and Svirčev, Zorica},
	month = may,
	year = {2016},
	pages = {66--76},
	file = {Drobac et al. - 2016 - Cyanobacteria and cyanotoxins in fishponds and the.pdf:C\:\\Users\\MTeam\\Zotero\\storage\\YCWPB6JP\\Drobac et al. - 2016 - Cyanobacteria and cyanotoxins in fishponds and the.pdf:application/pdf}
}

