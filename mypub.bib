	pages = {277--84},
	annote = {Hermens, Wim ThBenes, MartinRichter, RalfSpeijer, HanengEngland2004/05/25 05:00Biotechnol Appl Biochem. 2004 Jun;39(Pt 3):277-84.},
	file = {Biotechnol_Appl_Biochem_2004_39_p277_Hermens:C\:\\Users\\MTeam\\Zotero\\storage\\B2SZUH9M\\Biotechnol_Appl_Biochem_2004_39_p277_Hermens.pdf:application/pdf}
