	pages = {64--73},
	annote = {Liu, YuhongLord, HeatherMaciazek-Jurczyk, MalgorzataJolly, SukhsagarHussain, Mohamed AsimPawliszyn, JanuszengNetherlands2014/09/04 06:00J Chromatogr A. 2014 Oct 17;1364:64-73. doi: 10.1016/j.chroma.2014.08.042. Epub 2014 Aug 17.},
