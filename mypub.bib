	keywords = {Physicochemical Phenomena, Polyethylene/chemistry, Silicone Elastomers/chemistry, Polymers/*chemistry, Chemistry, Physical, Dimethylpolysiloxanes/chemistry, Permeability, Resins, Synthetic/chemistry, Silicones/chemistry},
	pages = {1344--51},
