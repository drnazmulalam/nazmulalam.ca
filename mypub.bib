	pages = {8223--8},
	annote = {Dandy, David SWu, PengGrainger, David WENGR01 EB000726/EB/NIBIB NIH HHS/R01 EB001473/EB/NIBIB NIH HHS/EB00726/EB/NIBIB NIH HHS/Research Support, N.I.H., Extramural2007/05/09 09:00Proc Natl Acad Sci U S A. 2007 May 15;104(20):8223-8. Epub 2007 May 7.},
	file = {PNAS-2007-Dandy-8223-8:C\:\\Users\\MTeam\\Zotero\\storage\\6DPC294R\\PNAS-2007-Dandy-8223-8.pdf:application/pdf}
