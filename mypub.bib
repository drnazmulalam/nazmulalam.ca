@article{conder_nondestructive_2003-1,
	title = {Nondestructive, {Minimal}-{Disturbance}, {Direct}-{Burial} {Solid}-{Phase} {Microextraction} {Fiber} {Technique} for {Measuring} {TNT} in {Sediment}},
	volume = {37},
	issn = {0013-936X},
	url = {http://dx.doi.org/10.1021/es0260770},
	doi = {10.1021/es0260770},
	abstract = {We explored a novel technique to deploy solid-phase microextraction (SPME) fibers to nondestructively measure the explosive compound 2,4,6-trinitrotoluene (TNT) and its nitroaromatic (NA) degradation products in laboratory sediment toxicity tests and field sediments in situ. SPME fibers within steel mesh envelopes were exposed statically via direct burial within sediment. Six fiber types (polymer coatings) were tested. Polyacrylate (PA) SPME fiber was sufficiently durable for this application, yielded the lowest detection limits, and exhibited a linear uptake relationship across toxicologically relevant sediment NA concentrations (100−2000 nmol/g dw (20−500 μg/g dw)). Temperature greatly influenced SPME absorption kinetics. Via evaluation of absorption at different temperatures, recommended sampling times needed to achieve steady-state equilibrium were 48 h for room temperatures (23−25 °C) and up to 7 d for cold (5 °C) temperatures. Although a comparison of TNT residues by SPMEs and TNT bioavailability and toxicity in sediments has not been completed, differences in SPME availability of TNT and its degradation products were found between two different TNT-spiked sediments. Our disposable SPME technique was slightly less expensive and as precise as the conventional extraction for total NAs and may prove to be a powerful exposure evaluation tool for assessing the ecological risk of these compounds.},
	number = {8},
	urldate = {2017-08-23},
	journal = {Environmental Science \& Technology},
	author = {Conder, Jason M. and La Point, Thomas W. and Lotufo, Guilherme R. and Steevens, Jeffery A.},
	month = apr,
	year = {2003},
	pages = {1625--1632},
	file = {ACS Full Text Snapshot:C\:\\Users\\MTeam\\Zotero\\storage\\AVSXQWB9\\es0260770.html:text/html;ACS Full Text PDF w/ Links:C\:\\Users\\MTeam\\Zotero\\storage\\EKQFISAM\\Conder et al. - 2003 - Nondestructive, Minimal-Disturbance, Direct-Burial.pdf:application/pdf}
}

@article{verbruggen_polyacrylate-coated_2000-1,
	title = {Polyacrylate-{Coated} {SPME} {Fibers} as a {Tool} {To} {Simulate} {Body} {Residues} and {Target} {Concentrations} of {Complex} {Organic} {Mixtures} for {Estimation} of {Baseline} {Toxicity}},
