	author = {Li, Dan and Zou, Juan and Cai, Pei-Shan and Xiong, Chao-Mei and Ruan, Jin-Lan},
	month = jun,
	year = {2016},
	keywords = {Clozapine, HPLC-UV, Magnetic octadecylsilane-polyacrylonitrile thin-film microextraction (ODS-PAN TFME), Plasma, Quetiapine, Therapeutic drug monitoring (TDM), Urine},
	pages = {319--328},
	file = {ScienceDirect Snapshot:C\:\\Users\\MTeam\\Zotero\\storage\\EEHHU725\\S0731708516301923.html:text/html;ScienceDirect Full Text PDF:C\:\\Users\\MTeam\\Zotero\\storage\\KHBKSFEA\\Li et al. - 2016 - Preparation of magnetic ODS-PAN thin-films for mic.pdf:application/pdf}
