@article{manago_reliable_2016,
	title = {A reliable {Raman}-spectroscopy-based approach for diagnosis, classification and follow-up of {B}-cell acute lymphoblastic leukemia},
	volume = {6},
	issn = {2045-2322 (Electronic) 2045-2322 (Linking)},
	shorttitle = {A reliable {Raman}-spectroscopy-based approach for diagnosis, classification and follow-up of {B}-cell acute lymphoblastic leukemia},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/27089853},
	doi = {10.1038/srep24821},
	abstract = {Acute lymphoblastic leukemia type B (B-ALL) is a neoplastic disorder that shows high mortality rates due to immature lymphocyte B-cell proliferation. B-ALL diagnosis requires identification and classification of the leukemia cells. Here, we demonstrate the use of Raman spectroscopy to discriminate normal lymphocytic B-cells from three different B-leukemia transformed cell lines (i.e., RS4;11, REH, MN60 cells) based on their biochemical features. In combination with immunofluorescence and Western blotting, we show that these Raman markers reflect the relative changes in the potential biological markers from cell surface antigens, cytoplasmic proteins, and DNA content and correlate with the lymphoblastic B-cell maturation/differentiation stages. Our study demonstrates the potential of this technique for classification of B-leukemia cells into the different differentiation/maturation stages, as well as for the identification of key biochemical changes under chemotherapeutic treatments. Finally, preliminary results from clinical samples indicate high consistency of, and potential applications for, this Raman spectroscopy approach.},
	journal = {Sci Rep},
	author = {Manago, S. and Valente, C. and Mirabelli, P. and Circolo, D. and Basile, F. and Corda, D. and De Luca, A. C.},
	year = {2016},
