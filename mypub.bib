	file = {c5an01786f:C\:\\Users\\MTeam\\Zotero\\storage\\37J79IP4\\c5an01786f.pdf:application/pdf}
}

@article{bao_isotopic_2016,
	title = {Isotopic exchange on solid-phase micro extraction fiber in sediment under stagnant conditions: {Implications} for field application of performance reference compound calibration},
	volume = {35},
	issn = {1552-8618 (Electronic) 0730-7268 (Linking)},
	shorttitle = {Isotopic exchange on solid-phase micro extraction fiber in sediment under stagnant conditions: {Implications} for field application of performance reference compound calibration},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/26678218},
	doi = {10.1002/etc.3345},
	abstract = {An overlooked issue for field application of in situ performance reference compound (PRC) calibration methods is the validity of the assumption that both the sorption of a target compound and desorption of its corresponding PRC follow the first-order kinetics with the same rate constants under stagnant conditions. In the present study, disposable polydimethylsiloxane fibers of 2 sizes (7 and 35 microm) impregnated with 8 (13) C-labeled or deuterated PRCs were statically deployed into different marine sediments, from which the kinetics for sorption of the target compounds and desorption of the PRCs were characterized. Nonsymmetrical profiles were observed for exchange of the target analytes and their corresponding PRCs in sediment under stagnant conditions. The hysteretic desorption of PRCs in the kinetic regime may be ascribed to the low chemical potential between the fiber and sediment porewater, which reflects the inability of water molecules to rapidly diffuse through sediment to solvate the PRCs in the aqueous layer around the fiber surface. A moderate correlation (r = 0.77 and r = 0.57, p {\textless} 0.05 for both regressions) between the PRC-calibrated equilibrium concentrations of 1,1-dichloro-2,2-bis-(chlorophenyl) ethylene (p,p'-DDE) and polychlorinated biphenyl (PCB)-153 and the lipid normalized levels in worms (Neanthes arenaceodentata) was obtained in co-exposure tests under simulating field conditions, probably resulting from slightly overestimated bioavailability because of the hysteretic desorption of PRCs and toxic effects. Environ Toxicol Chem 2016;35:1978-1985. (c) 2015 SETAC.},
	number = {8},
	journal = {Environ Toxicol Chem},
	author = {Bao, L. J. and Wu, X. and Jia, F. and Zeng, E. Y. and Gan, J.},
	month = aug,
	year = {2016},
	pages = {1978--85},
	annote = {Bao, Lian-JunWu, XiaoqinJia, FangZeng, Eddy YGan, JayENG2015/12/19 06:00Environ Toxicol Chem. 2016 Aug;35(8):1978-85. doi: 10.1002/etc.3345. Epub 2016 Jul 14.},
	file = {Bao_et_al-2016-Environmental_Toxicology_and_Chemistry:C\:\\Users\\MTeam\\Zotero\\storage\\VRT7VXC7\\Bao_et_al-2016-Environmental_Toxicology_and_Chemistry.pdf:application/pdf}
