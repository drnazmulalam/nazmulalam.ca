	file = {Full Text PDF:C\:\\Users\\MTeam\\Zotero\\storage\\BDQMK33J\\Morgan et al. - 1986 - Pharmacokinetics of intravenous and oral salbutamo.pdf:application/pdf;Snapshot:C\:\\Users\\MTeam\\Zotero\\storage\\N826PH33\\abstract.html:text/html}
}

