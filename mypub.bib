@article{cui_mass_2012,
	title = {The mass transfer dynamics of hollow fiber liquid-phase microextraction and its application for rapid analysis of biological samples},
	volume = {1266},
	issn = {1873-3778 (Electronic) 0021-9673 (Linking)},
	shorttitle = {The mass transfer dynamics of hollow fiber liquid-phase microextraction and its application for rapid analysis of biological samples},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/23116799},
	doi = {10.1016/j.chroma.2012.10.024},
	abstract = {Hollow fiber liquid-phase microextraction (HF-LPME) has been demonstrated to potentially become a mainstream sample preparation technique for complex samples. Nevertheless, the need for a relatively long extraction time is considered to be the major disadvantage of this method. Lengthy extractions may cause the loss of the extraction phase and may change the contents of biological samples via the action of enzymes. Therefore, control calibrations for particular biological systems must be made. In this study, a theoretical model of the mass transfer dynamics of two-phase HF-LPME was proposed, and the kinetic calibration (KC) of this method for plasma and urine samples was validated. The theoretical results were validated by examining the kinetics of the extraction and back-extraction processes of HF-LPME. The KC-HF-LPME method was successfully used to correct for matrix effects in plasma and urine samples during flunitrazepam analysis. The free amount of flunitrazepam was extracted from plasma for 10 min and analyzed by gas chromatography/mass spectrometry. The amount of pre-added standard and the standard remaining in the extraction phase after extraction were used for the quantification of flunitrazepam in plasma and urine samples. The new method not only significantly shortens the extraction time but also provides a new opportunity to determine the free concentration of analyte in biological systems.},
	journal = {J Chromatogr A},
	author = {Cui, S. and Ouyang, G. and Duan, G. and Hou, J. and Luan, T. and Zhang, X.},
	month = nov,
	year = {2012},
	keywords = {Calibration, Kinetics, Limit of Detection, Humans, *Models, Theoretical, Reproducibility of Results, Female, Liquid Phase Microextraction/*methods, Linear Models, Flunitrazepam/blood/urine, Gas Chromatography-Mass Spectrometry/methods},
	pages = {10--6},
	annote = {Cui, ShufenOuyang, GangfengDuan, GuijiaoHou, JinxingLuan, TiangangZhang, XuengResearch Support, Non-U.S. Gov'tNetherlands2012/11/03 06:00J Chromatogr A. 2012 Nov 30;1266:10-6. doi: 10.1016/j.chroma.2012.10.024. Epub 2012 Oct 16.},
	file = {The-mass-transfer-dynamics-of-hollow-fiber-liquid-phase-microextraction-and-its-application-for-rapid-analysis-of-biological-samples_2012_Journal-of-Chromatography-A:C\:\\Users\\MTeam\\Zotero\\storage\\R69F9JUA\\The-mass-transfer-dynamics-of-hollow-fiber-liquid-phase-microextraction-and-its-application-for-rapid-analysis-of-biological-samples_2012_Journal-of-C.pdf:application/pdf}
