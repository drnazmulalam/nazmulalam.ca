	keywords = {Environmental Monitoring/*methods, Polyethylene/*chemistry, Limit of Detection, Models, Chemical, Water Pollutants, Chemical/*analysis, Polychlorinated Biphenyls/*analysis, Rivers/*chemistry, Water/chemistry, Switzerland},
	pages = {268--76},
