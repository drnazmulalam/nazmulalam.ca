@article{chen_aptamer_2016,
	title = {Aptamer functionalized hydrophilic polymer monolith with gold nanoparticles modification for the sensitive detection of human alpha-thrombin},
	volume = {154},
	issn = {1873-3573 (Electronic) 0039-9140 (Linking)},
	shorttitle = {Aptamer functionalized hydrophilic polymer monolith with gold nanoparticles modification for the sensitive detection of human alpha-thrombin},
