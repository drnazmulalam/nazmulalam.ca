	pmcid = {PMC5343551},
	pages = {2890--2898},
	file = {PubMed Central Full Text PDF:C\:\\Users\\MTeam\\Zotero\\storage\\QKH52DTS\\Timmer and Droge - 2017 - Sorption of Cationic Surfactants to Artificial Cel.pdf:application/pdf}
