@article{perez_diffusive_2016,
	title = {Diffusive {Milli}-{Gels} ({DMG}) for in situ assessment of metal bioavailability: {A} comparison with labile metal measurement using {Chelex} columns and acute toxicity to {Ceriodaphnia} dubia for copper in freshwaters},
	volume = {164},
	issn = {1879-1298 (Electronic) 0045-6535 (Linking)},
	shorttitle = {Diffusive {Milli}-{Gels} ({DMG}) for in situ assessment of metal bioavailability: {A} comparison with labile metal measurement using {Chelex} columns and acute toxicity to {Ceriodaphnia} dubia for copper in freshwaters},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/27568367},
	doi = {10.1016/j.chemosphere.2016.08.006},
	abstract = {Fluctuations in concentrations of bioavailable metals occur in most natural waters. In situ measurements are desirable to predict risks of adverse effects to aquatic organisms. We evaluated Diffusive Milli-Gels (DMG), a new in situ passive sampler, for assessing the bioavailability and toxicity of copper in waters exhibiting a wide range of characteristics. The performance was compared to an established Chelex-column method that measures labile copper concentrations by discrete sampling, and the ability to predict acute toxicity to the cladoceran, Ceriodaphnia dubia. The labile copper concentrations measured by the DMG and Chelex-column methods decreased with increasing dissolved organic carbon (DOC) (1.9-15 mg L-1) and hardness (21-270 mg CaCO3 L-1 hardness), with 20-70\% of total dissolved copper being present as labile copper. Toxicity decreased with increasing DOC and hardness. Strong linear relationships existed between the EC50 for C. dubia and DOC, and when the EC50 was related to either the labile copper concentrations measured by DMG (r2 = 0.874) or the Chelex column (0.956) methods. The study demonstrates that the DMG passive sampler is a relevant tool for the in situ assessment of environmental risks posed by metals whose toxicity is strongly influenced by speciation.},
	journal = {Chemosphere},
	author = {Perez, M. and Simpson, S. L. and Lespes, G. and King, J. J. and Adams, M. S. and Jarolimek, C. V. and Grassl, B. and Schaumloffel, D.},
	month = dec,
	year = {2016},
	pages = {7--13},
	annote = {Perez, MagaliSimpson, Stuart LLespes, GaetaneKing, Josh JAdams, Merrin SJarolimek, Chad VGrassl, BrunoSchaumloffel, DirkENGEngland2016/08/29 06:00Chemosphere. 2016 Dec;164:7-13. doi: 10.1016/j.chemosphere.2016.08.006. Epub 2016 Aug 26.},
	file = {1-s2.0-S0045653516310232-main:C\:\\Users\\MTeam\\Zotero\\storage\\FG4B6KPK\\1-s2.0-S0045653516310232-main.pdf:application/pdf}
}

