	keywords = {Temperature, Water Pollutants, Chemical/*analysis, Polychlorinated Biphenyls/*analysis, Environmental Monitoring/*instrumentation/methods, Dioxins/*analysis, Tetrachlorodibenzodioxin, Water/chemistry},
	pages = {18--27},
