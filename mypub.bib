@article{booij_passive_2015,
	title = {Passive samplers of hydrophobic organic chemicals reach equilibrium faster in the laboratory than in the field},
	volume = {98},
	issn = {1879-3363 (Electronic) 0025-326X (Linking)},
	shorttitle = {Passive samplers of hydrophobic organic chemicals reach equilibrium faster in the laboratory than in the field},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/26187397},
	doi = {10.1016/j.marpolbul.2015.07.007},
	abstract = {The use of passive sampling methods for monitoring hydrophobic organic chemicals frequently requires the determination of equilibration times and partition coefficients in the laboratory. These experiments are often carried out by exposing passive samplers in a finite water volume, and errors are easily made when the obtained results are applied to the field, where water volumes are essentially infinite. The effect of water volume on the equilibration rate constant is discussed, using a mechanistic model. Application of this model to two literature reports illustrates that aqueous concentrations in the field may be underestimated by a factor of 10 or more, when the water volume effect is neglected. Finally, it is shown that the concept of "sorption capacity" (sampler mass times partition coefficient) allows for a more intuitive understanding of the passive sampling process in small and large water volumes, which may reduce the risk of laboratory-field extrapolation errors.},
	journal = {Mar Pollut Bull},
	author = {Booij, K. and Tucca, F.},
	month = sep,
	year = {2015},
	pages = {365--7},
	annote = {Booij, KeesTucca, FelipeengResearch Support, Non-U.S. Gov'tEngland2015/07/19 06:00Mar Pollut Bull. 2015 Sep 15;98(1-2):365-7. doi: 10.1016/j.marpolbul.2015.07.007. Epub 2015 Jul 15.},
	file = {Booij et al_2015_Passive samplers of hydrophobic organic chemicals reach equilibrium faster in the laboratory than in the field:C\:\\Users\\MTeam\\Zotero\\storage\\G9A874BX\\Booij et al_2015_Passive samplers of hydrophobic organic chemicals reach equilibrium faster in the laboratory than in the field.pdf:application/pdf}
}

@article{bozsak_optimization_2015,
	title = {Optimization of {Drug} {Delivery} by {Drug}-{Eluting} {Stents}},
	volume = {10},
	issn = {1932-6203 (Electronic) 1932-6203 (Linking)},
	shorttitle = {Optimization of {Drug} {Delivery} by {Drug}-{Eluting} {Stents}},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/26083626},
	doi = {10.1371/journal.pone.0130182},
	abstract = {Drug-eluting stents (DES), which release anti-proliferative drugs into the arterial wall in a controlled manner, have drastically reduced the rate of in-stent restenosis and revolutionized the treatment of atherosclerosis. However, late stent thrombosis remains a safety concern in DES, mainly due to delayed healing of the endothelial wound inflicted during DES implantation. We present a framework to optimize DES design such that restenosis is inhibited without affecting the endothelial healing process. To this end, we have developed a computational model of fluid flow and drug transport in stented arteries and have used this model to establish a metric for quantifying DES performance. The model takes into account the multi-layered structure of the arterial wall and incorporates a reversible binding model to describe drug interaction with the cells of the arterial wall. The model is coupled to a novel optimization algorithm that allows identification of optimal DES designs. We show that optimizing the period of drug release from DES and the initial drug concentration within the coating has a drastic effect on DES performance. Paclitaxel-eluting stents perform optimally by releasing their drug either very rapidly (within a few hours) or very slowly (over periods of several months up to one year) at concentrations considerably lower than current DES. In contrast, sirolimus-eluting stents perform optimally only when drug release is slow. The results offer explanations for recent trends in the development of DES and demonstrate the potential for large improvements in DES design relative to the current state of commercial devices.},
	number = {6},
	journal = {PLoS One},
	author = {Bozsak, F. and Gonzalez-Rodriguez, D. and Sternberger, Z. and Belitz, P. and Bewley, T. and Chomaz, J. M. and Barakat, A. I.},
	year = {2015},
