	title = {Detection of free and covalently bound microcystins in different tissues (liver, intestines, gills, and muscles) of rainbow trout ({Oncorhynchus} mykiss) by liquid chromatography–tandem mass spectrometry: {Method} characterization},
	volume = {185},
