@article{sivanesan_reproducible_2015,
	title = {Reproducible and label-free biosensor for the selective extraction and rapid detection of proteins in biological fluids},
	volume = {13},
	issn = {1477-3155 (Electronic) 1477-3155 (Linking)},
	shorttitle = {Reproducible and label-free biosensor for the selective extraction and rapid detection of proteins in biological fluids},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/26104688},
	doi = {10.1186/s12951-015-0102-8},
	abstract = {Erythropoietin (EPO), a glycoprotein hormone of approximately 34 kDa, is an important hematopoietic growth factor, mainly produced in the kidney and controls the number of red blood cells circulating in the blood stream. Sensitive and rapid recombinant human EPO (rHuEPO) detection tools that improve on the current laborious EPO detection techniques are in high demand for both clinical and sports industry. A sensitive aptamer-functionalized biosensor (aptasensor) has been developed by controlled growth of gold nanostructures (AuNS) over a gold substrate (pAu/AuNS). The aptasensor selectively binds to rHuEPO and, therefore, was used to extract and detect the drug from horse plasma by surface enhanced Raman spectroscopy (SERS). Due to the nanogap separation between the nanostructures, the high population and distribution of hot spots on the pAu/AuNS substrate surface, strong signal enhancement was acquired. By using wide area illumination (WAI) setting for the Raman detection, a low RSD of 4.92\% over 150 SERS measurements was achieved. The significant reproducibility of the new biosensor addresses the serious problem of SERS signal inconsistency that hampers the use of the technique in the field. The WAI setting is compatible with handheld Raman devices. Therefore, the new aptasensor can be used for the selective extraction of rHuEPO from biological fluids and subsequently screened with handheld Raman spectrometer for SERS based in-field protein detection.},
	journal = {J Nanobiotechnology},
	author = {Sivanesan, A. and Izake, E. L. and Agoston, R. and Ayoko, G. A. and Sillence, M.},
	year = {2015},
	keywords = {Limit of Detection, Humans, Animals, Reproducibility of Results, Gold/*chemistry, Aptamers, Nucleotide/*chemistry, Biosensing Techniques/*methods, Erythropoietin/*blood/isolation \& purification, Horses/*blood, Nanostructures/*chemistry, Recombinant Proteins/blood/isolation \& purification, Spectrum Analysis, Raman/methods},
	pages = {43},
