@article{santner_numerical_2015,
	title = {Numerical evaluation of lateral diffusion inside diffusive gradients in thin films samplers},
	volume = {49},
	issn = {1520-5851 (Electronic) 0013-936X (Linking)},
	shorttitle = {Numerical evaluation of lateral diffusion inside diffusive gradients in thin films samplers},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/25877251},
	doi = {10.1021/acs.est.5b00134},
	abstract = {Using numerical simulation of diffusion inside diffusive gradients in thin films (DGT) samplers, we show that the effect of lateral diffusion inside the sampler on the solute flux into the sampler is a nonlinear function of the diffusion layer thickness and the physical sampling window size. In contrast, earlier work concluded that this effect was constant irrespective of parameters of the sampler geometry. The flux increase caused by lateral diffusion inside the sampler was determined to be approximately 8.8\% for standard samplers, which is considerably lower than the previous estimate of approximately 20\%. Lateral diffusion is also propagated to the diffusive boundary layer (DBL), where it leads to a slightly stronger decrease in the mass uptake than suggested by the common 1D diffusion model that is applied for evaluating DGT results. We introduce a simple correction procedure for lateral diffusion and demonstrate how the effect of lateral diffusion on diffusion in the DBL can be accounted for. These corrections often result in better estimates of the DBL thickness (delta) and the DGT-measured concentration than earlier approaches and will contribute to more accurate concentration measurements in solute monitoring in waters.},
	number = {10},
	journal = {Environ Sci Technol},
	author = {Santner, J. and Kreuzeder, A. and Schnepf, A. and Wenzel, W. W.},
	month = may,
	year = {2015},
	keywords = {Environmental Monitoring/*methods, Diffusion, *Models, Theoretical, Solutions, Water Pollutants, Chemical/*analysis, Water/*chemistry, Computer Simulation},
	pages = {6109--16},
	annote = {Santner, JakobKreuzeder, AndreasSchnepf, AndreaWenzel, Walter WENGResearch Support, Non-U.S. Gov't2015/04/17 06:00Environ Sci Technol. 2015 May 19;49(10):6109-16. doi: 10.1021/acs.est.5b00134. Epub 2015 Apr 30.},
	annote = {Santner, JakobKreuzeder, AndreasSchnepf, AndreaWenzel, Walter WENGResearch Support, Non-U.S. Gov't2015/04/17 06:00Environ Sci Technol. 2015 May 19;49(10):6109-16. doi: 10.1021/acs.est.5b00134. Epub 2015 Apr 30.},
	file = {acs%2Eest%2E5b00134:C\:\\Users\\MTeam\\Zotero\\storage\\ZCTD4H7W\\acs%2Eest%2E5b00134.pdf:application/pdf;es5b00134:C\:\\Users\\MTeam\\Zotero\\storage\\XPWJ2VXS\\es5b00134.pdf:application/pdf}
