}

@article{ortiz_high_2017,
	title = {A high throughput targeted and non-targeted method for the analysis of microcystins and anatoxin-{A} using on-line solid phase extraction coupled to liquid chromatography–quadrupole time-of-flight high resolution mass spectrometry},
