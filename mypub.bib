@article{tassa_binding_2010,
	title = {Binding affinity and kinetic analysis of targeted small molecule-modified nanoparticles},
	volume = {21},
	issn = {1520-4812 (Electronic) 1043-1802 (Linking)},
	shorttitle = {Binding affinity and kinetic analysis of targeted small molecule-modified nanoparticles},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/20028085},
	doi = {10.1021/bc900438a},
	abstract = {Nanoparticles bearing surface-conjugated targeting ligands are increasingly being explored for a variety of biomedical applications. The multivalent conjugation of targeting ligands on the surface of nanoparticles is presumed to enhance binding to the desired target. However, given the complexities inherent in the interactions of nanoparticle surfaces with proteins, and the structural diversity of nanoparticle scaffolds and targeting ligands, our understanding of how conjugation of targeting ligands affects nanoparticle binding remains incomplete. Here, we use surface plasmon resonance (SPR) to directly and quantitatively study the affinity and binding kinetics of nanoparticles that display small molecules conjugated to their surface. We studied the interaction between a single protein target and a structurally related series of targeting ligands whose intrinsic affinity varies over a 4500-fold range and performed SPR at protein densities that reflect endogenous receptor densities. We report that even weak small molecule targeting ligands can significantly enhance target-specific avidity (by up to 4 orders of magnitude) through multivalent interactions and also observe a much broader range of kinetic effects than has been previously reported. Quantitative measurement of how the affinity and kinetics of nanoparticle binding vary as a function of different surface conjugations is a rapid, generalizable approach to nanoparticle characterization that can inform the design of nanoparticles for biomedical applications.},
	number = {1},
	journal = {Bioconjug Chem},
	author = {Tassa, C. and Duffner, J. L. and Lewis, T. A. and Weissleder, R. and Schreiber, S. L. and Koehler, A. N. and Shaw, S. Y.},
	month = jan,
	year = {2010},
	keywords = {Kinetics, Protein Binding, Ligands, *Molecular Probe Techniques, Nanoparticles/*analysis/*chemistry, Proteins/chemistry/*metabolism, Receptors, Cell Surface/metabolism, Surface Plasmon Resonance},
	pages = {14--9},
	annote = {Tassa, CarlosDuffner, Jay LLewis, Timothy AWeissleder, RalphSchreiber, Stuart LKoehler, Angela NShaw, Stanley YengCA119349/CA/NCI NIH HHS/HL077186/HL/NHLBI NIH HHS/HL80731/HL/NHLBI NIH HHS/K08 HL077186/HL/NHLBI NIH HHS/K08 HL077186-05/HL/NHLBI NIH HHS/U01 HL080731/HL/NHLBI NIH HHS/U01 HL080731-05/HL/NHLBI NIH HHS/U54 CA119349/CA/NCI NIH HHS/U54 CA119349-050003/CA/NCI NIH HHS/Research Support, N.I.H., ExtramuralResearch Support, Non-U.S. Gov't2009/12/24 06:00Bioconjug Chem. 2010 Jan;21(1):14-9. doi: 10.1021/bc900438a.},
