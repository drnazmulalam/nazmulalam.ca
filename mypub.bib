	pages = {3997--4008},
	annote = {Assoumani, ACoquery, MLiger, LMazzella, NMargoum, CENGEvaluation StudiesResearch Support, Non-U.S. Gov'tGermany2014/09/19 06:00Environ Sci Pollut Res Int. 2015 Mar;22(6):3997-4008. doi: 10.1007/s11356-014-3590-4. Epub 2014 Sep 19.},
