	month = aug,
	year = {2010},
	keywords = {HS-SPME, 2-Acetyl-1-pyrroline, Convenient synthesis, GC–MS–MS, Rice, SIDA},
	pages = {148--155},
	file = {ScienceDirect Snapshot:C\:\\Users\\MTeam\\Zotero\\storage\\RJVP8EBH\\S0003267010008913.html:text/html;ScienceDirect Full Text PDF:C\:\\Users\\MTeam\\Zotero\\storage\\9NRZWVE7\\Maraval et al. - 2010 - Quantification of 2-acetyl-1-pyrroline in rice by .pdf:application/pdf}
}

